1.string:
	    Scanner sc=new Scanner(System.in
	    String str=sc.next();
		System.out.println(str);

2.using nextline:
     Scanner sc=new Scanner(System.in);
	    String str=sc.nextLine();
		System.out.println(str);

3.int:
    	 Scanner sc=new Scanner(System.in);
	    int num=sc.nextInt();
		System.out.println(num);

4.short:
        Scanner sc=new Scanner(System.in);
	    Short num=sc.nextShort();
		System.out.println(num);

5.byte:
     	Scanner sc=new Scanner(System.in);
	    byte num=sc.nextByte();
		System.out.println(num);

6.float:

 float num=sc.nextFloat();
	System.out.println(num);

7.long:
 long num=sc.nextLong();
		System.out.println(num);

