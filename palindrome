import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	  int a=sc.nextInt();
	  int rev=0;
	  int temp=a;

	  while(a>0){
	      int r=a%10;
	      rev=rev*10+r;
	      a=a/10;
	  }
	  if(temp==rev){
	      System.out.println("palindrome");
 
	  }
	  else{
	       System.out.println(" not palindrome");
	  }
	 
	
	}
}
