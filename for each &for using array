import java.util.*;

public class Main {
    public static void main(String[] args) {
      int arr[]={1,2,3};
       for(int i=0;i<arr.length;i++){//for loop
        System.out.print(arr[i]+ " ");
        }
        for(int x:arr){//for each
          System.out.println(x + " ");
       }
    }
}   
