import java.util.*;

public class Main {
    public static void main(String[] args) {
     Scanner sc=new Scanner(System.in);
     int size=sc.nextInt();
     int arr[]=new int[size];
     int mul=1;
     for(int i=0;i<size;i++){
       arr[i]=sc.nextInt();
     }
     for(int i=0;i<size;i++){
    
       mul=mul*arr[i];
     }System.out.println(mul);
    }
}   
